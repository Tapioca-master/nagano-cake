<h1>配送先編集</h1>
<%= form_for(@address) do |f| %>
<table>
    <tr>
        <td>郵便番号（ハイフンあり7桁）</td>
        <td><%= f.text_field :postal_code %></td>
    </tr>
    <tr>
    	<td>住所</td>
    	<td><%= f.text_field :address %></td>
    </tr>
    <tr>
    	<td>宛名</td>
    	<td><%= f.text_field :name %></td>
    </tr>
</table>
<%= f.submit "編集する" %>
<% end %>