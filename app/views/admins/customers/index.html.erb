<h1>会員一覧画面</h1>
<table>
    <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
    </tr>
    <% @customers.each do |customer| %>
    <tr>
    	<td><%= customer.id %></td>
    	<td><%= link_to "#{customer.name_first}" + "#{customer.name_last}", admins_customer_path(customer) %></td>
    	<td><%= customer.email %></td>
    	<td><%= customer.is_active ? "有効" : "退会済" %></td>
    </tr>
    <% end %>
</table>